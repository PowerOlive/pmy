[
  {
    "regexpLeft": "^make ",
    "regexpRight": "",
    "command": "cat Makefile"
  },
  {
    "regexpLeft": "^cd +(?P<path>.+)$",
    "command": "ls -1 ${path}"
  },
  {
    "regexpLeft": "git +add +",
    "command": "git status --short | git status -s | grep -Ev \"^(A|C|D|M|R|T|U|X|B)\" | cut -c4-"
  },
  {
    "regexpLeft": "^export ",
    "command": "printenv"
  },
  {
    "regexpLeft": "^(vi||vim|nvim) ",
    "command": "find . -maxdepth 2"
  }
]
