      "bufferRight": "[]"
    },
    {
      "regexpLeft": "^python.* ",
      "command": "\\find . | egrep py$",
      "bufferLeft": "[]",
      "bufferRight": "[]"
    },
    {
      "regexpLeft": "^go ",
      "command": "\\find . | egrep go$",
      "bufferLeft": "[]",
      "bufferRight": "[]"
    },
    {
      "regexpLeft": "^make ",
      "regexpRight": "",
      "command": "cat Makefile",
      "bufferLeft": "[]",
      "bufferRight": "[]"
    },
    {
      "regexpLeft": "git +add +",
      "command": "git status --short | git status -s | grep -Ev \"^(A|C|D|M|R|T|U|X|B)\" | cut -c4-",
      "bufferLeft": "[]",
      "bufferRight": "[]"
    },
    {
      "regexpLeft": "^export ",
      "command": "printenv",
      "bufferLeft": "[]",
      "bufferRight": "[]"
    },
    {
      "regexpLeft": "(?P<cmd>.+)\\.for",
      "command": "echo ''",
      "bufferLeft": "for x in $(<cmd>); do ",
      "bufferRight": "; done"
    },
    {
      "regexpLeft": "^(vi||vim|nvim) ",
      "command": "find . -maxdepth 2",
      "bufferLeft": "[]",
      "bufferRight": "[]"
    }
