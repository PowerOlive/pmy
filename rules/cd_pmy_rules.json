[
    {
      "regexpLeft": "^cd +(?P<path>([A-Za-z_\\-.]*/)*)(?P<query>[A-Za-z_\\-.]*)$",
      "cmdGroups": [
        {
          "tag": "",
          "stmt":  "command ls -F -1 <path> | egrep '/$'",
          "after": "awk '{print $0}'"
        }
      ],
      "fuzzyFinderCmd": "fzf -0 -1 -q \"<query>\"",
      "bufferLeft": "cd <path>",
      "bufferRight": "[]"
    }
]
